    "@types/zen-observable": "^0.8.0",
    "benchmark": "1.0.0",
    "bluebird": "^2",
    "bower-json": "^0.6.0",
    "browserify": "^16.2.3",
    "browserstack-runner": "^0.3.8",
    "chai": "^3.3.0",
    "es6-promise": "^3.0.2",
    "escodegen": "^1.11.1",
    "eslint": "^1.10.3",
    "eslint-config-defaults": "^7.1.1",
    "esprima": "^4.0.1",
